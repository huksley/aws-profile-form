{
  "name": "aws-profile-form",
  "version": "1.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "build": "chamber exec find-face/test1 -- webpack --mode production",
    "build:dev": "chamber exec find-face/test1 -- nodemon -w webpack.config.js --exec webpack",
    "dev": "chamber exec find-face/test1 -- nodemon -w webpack.config.js -w src/firebase-messaging-sw.js --exec webpack-dev-server --mode development",
    "deploy": "chamber exec find-face/test1 -- yarn s3",
    "s3": "aws s3 sync --acl public-read dist/ s3://${WEBAPP_BUCKET}/"
  },
  "dependencies": {
    "bulma": "^0.7.4",
    "firebase": "^6.0.2",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-bulma-components": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/fontawesome-free": "^5.8.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.0",
    "react-dom": "^16.8.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  }
}
